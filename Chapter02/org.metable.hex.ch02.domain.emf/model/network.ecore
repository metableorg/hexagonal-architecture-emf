<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="network" nsURI="http://org.metable.hex.ch02.domain.network.emf/network"
    nsPrefix="org.metable.hex.ch02.domain.network.emf">
  <eClassifiers xsi:type="ecore:EClass" name="Router" eSuperTypes="#//IRouter">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="routerId" eType="#//RouterId"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//RouterType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="switch" eType="#//Switch"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EDataType" name="RouterType" instanceClassName="org.metable.hex.ch02.domain.valueobject.RouterType"/>
  <eClassifiers xsi:type="ecore:EClass" name="IRouter" instanceClassName="org.metable.hex.ch02.domain.entity.Router"
      abstract="true"/>
  <eClassifiers xsi:type="ecore:EDataType" name="RouterId" instanceClassName="org.metable.hex.ch02.domain.emf.network.impl.RouterIdImpl"/>
  <eClassifiers xsi:type="ecore:EDataType" name="SwitchType" instanceClassName="org.metable.hex.ch02.domain.valueobject.SwitchType"/>
  <eClassifiers xsi:type="ecore:EDataType" name="SwitchId" instanceClassName="org.metable.hex.ch02.domain.emf.network.impl.SwitchIdImpl"/>
  <eClassifiers xsi:type="ecore:EDataType" name="Network" instanceClassName="org.metable.hex.ch02.domain.valueobject.Network"/>
  <eClassifiers xsi:type="ecore:EDataType" name="IP" instanceClassName="org.metable.hex.ch02.domain.valueobject.IP"/>
  <eClassifiers xsi:type="ecore:EDataType" name="Protocol" instanceClassName="org.metable.hex.ch02.domain.valueobject.Protocol"/>
  <eClassifiers xsi:type="ecore:EClass" name="Switch" eSuperTypes="#//ISwitch">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="networks" upperBound="-1"
        eType="#//Network"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//SwitchType"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="switchId" eType="#//SwitchId"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="ipAddress" eType="#//IP"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ISwitch" instanceClassName="org.metable.hex.ch02.domain.entity.Switch"
      abstract="true" interface="true"/>
</ecore:EPackage>
